      "cell_type": "code",
      "source": [
        "# ==============================================================\n",
        "# ğŸ–ï¸ SIGN LANGUAGE RECOGNITION USING CNN (FULL PIPELINE)\n",
        "# ==============================================================\n",
        "\n",
        "# ğŸ“¦ Step 1 â€” Install & Import Libraries\n",
        "!pip install kagglehub -q\n",
        "\n",
        "import kagglehub, os, shutil\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import tensorflow as tf\n",
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import Conv2D, MaxPooling2D, Flatten, Dense, Dropout, BatchNormalization\n",
        "from tensorflow.keras.utils import to_categorical\n",
        "from tensorflow.keras.preprocessing.image import ImageDataGenerator\n",
        "from sklearn.metrics import classification_report, confusion_matrix\n",
        "from sklearn.model_selection import train_test_split\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ“¥ Step 2 â€” Download Sign Language MNIST Dataset using kagglehub\n",
        "# ==============================================================\n",
        "\n",
        "path = kagglehub.dataset_download(\"datamunge/sign-language-mnist\")\n",
        "print(\"âœ… Dataset downloaded to:\", path)\n",
        "\n",
        "# Copy CSVs to working directory\n",
        "os.makedirs(\"/content/sign_mnist_data\", exist_ok=True)\n",
        "shutil.copy(os.path.join(path, \"sign_mnist_train.csv\"), \"/content/sign_mnist_data/\")\n",
        "shutil.copy(os.path.join(path, \"sign_mnist_test.csv\"), \"/content/sign_mnist_data/\")\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ§  Step 3 â€” Load and Prepare the Data\n",
        "# ==============================================================\n",
        "\n",
        "train_df = pd.read_csv(\"/content/sign_mnist_data/sign_mnist_train.csv\")\n",
        "test_df  = pd.read_csv(\"/content/sign_mnist_data/sign_mnist_test.csv\")\n",
        "\n",
        "print(\"Train shape:\", train_df.shape)\n",
        "print(\"Test shape:\", test_df.shape)\n",
        "\n",
        "# Split into features and labels\n",
        "X_train = train_df.iloc[:, 1:].values\n",
        "y_train = train_df.iloc[:, 0].values\n",
        "X_test = test_df.iloc[:, 1:].values\n",
        "y_test = test_df.iloc[:, 0].values\n",
        "\n",
        "# Normalize and reshape\n",
        "X_train = X_train.reshape(-1, 28, 28, 1).astype(\"float32\") / 255.0\n",
        "X_test = X_test.reshape(-1, 28, 28, 1).astype(\"float32\") / 255.0\n",
        "\n",
        "# One-hot encode labels\n",
        "y_train = to_categorical(y_train, num_classes=25)\n",
        "y_test = to_categorical(y_test, num_classes=25)\n",
        "\n",
        "# Split a validation set from training data\n",
        "X_train, X_val, y_train, y_val = train_test_split(X_train, y_train, test_size=0.2, random_state=42)\n",
        "\n",
        "print(\"Training samples:\", X_train.shape)\n",
        "print(\"Validation samples:\", X_val.shape)\n",
        "print(\"Testing samples:\", X_test.shape)\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ¨ Step 4 â€” Data Augmentation\n",
        "# ==============================================================\n",
        "\n",
        "datagen = ImageDataGenerator(\n",
        "    rotation_range=10,\n",
        "    zoom_range=0.1,\n",
        "    width_shift_range=0.1,\n",
        "    height_shift_range=0.1\n",
        ")\n",
        "datagen.fit(X_train)\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ§© Step 5 â€” Define CNN Model (with BatchNorm & Dropout)\n",
        "# ==============================================================\n",
        "\n",
        "def build_model(conv_filters=64, dense_units=256, dropout_rate=0.3):\n",
        "    model = Sequential([\n",
        "        Conv2D(conv_filters, (3,3), activation='relu', input_shape=(28,28,1)),\n",
        "        BatchNormalization(),\n",
        "        MaxPooling2D(2,2),\n",
        "        Conv2D(conv_filters*2, (3,3), activation='relu'),\n",
        "        BatchNormalization(),\n",
        "        MaxPooling2D(2,2),\n",
        "        Flatten(),\n",
        "        Dense(dense_units, activation='relu'),\n",
        "        Dropout(dropout_rate),\n",
        "        Dense(25, activation='softmax')\n",
        "    ])\n",
        "    model.compile(optimizer='adam', loss='categorical_crossentropy', metrics=['accuracy'])\n",
        "    return model\n",
        "\n",
        "# ==============================================================\n",
        "# âš™ï¸ Step 6 â€” Hyperparameter Tuning (simple grid search)\n",
        "# ==============================================================\n",
        "\n",
        "best_acc = 0\n",
        "best_model = None\n",
        "filters_options = [32, 64]\n",
        "dense_units_options = [128, 256]\n",
        "dropout_options = [0.3, 0.4]\n",
        "\n",
        "for f in filters_options:\n",
        "    for d in dense_units_options:\n",
        "        for dr in dropout_options:\n",
        "            print(f\"\\nğŸš€ Training model with filters={f}, dense={d}, dropout={dr}\")\n",
        "            model = build_model(f, d, dr)\n",
        "            history = model.fit(\n",
        "                datagen.flow(X_train, y_train, batch_size=64),\n",
        "                validation_data=(X_val, y_val),\n",
        "                epochs=5,\n",
        "                verbose=1\n",
        "            )\n",
        "            val_acc = max(history.history['val_accuracy'])\n",
        "            if val_acc > best_acc:\n",
        "                best_acc = val_acc\n",
        "                best_model = model\n",
        "                print(f\"âœ… New best model with val_acc={best_acc:.4f}\")\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ§ª Step 7 â€” Evaluate on Test Data\n",
        "# ==============================================================\n",
        "\n",
        "test_loss, test_acc = best_model.evaluate(X_test, y_test, verbose=0)\n",
        "print(f\"\\nğŸ¯ Test Accuracy: {test_acc:.4f}\")\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ“Š Step 8 â€” Classification Report\n",
        "# ==============================================================\n",
        "\n",
        "y_pred = np.argmax(best_model.predict(X_test), axis=1)\n",
        "y_true = np.argmax(y_test, axis=1)\n",
        "\n",
        "print(\"\\nClassification Report:\\n\")\n",
        "print(classification_report(y_true, y_pred))\n",
        "\n",
        "# ==============================================================\n",
        "# ğŸ’¾ Step 9 â€” Save Model\n",
        "# ==============================================================\n",
        "\n",
        "best_model.save(\"/content/sign_language_cnn_model.h5\")\n",
        "print(\"âœ… Model saved as sign_language_cnn_model.h5\")\n"
      ],
